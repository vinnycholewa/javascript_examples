let raceNumber = Math.floor(Math.random() * 1000);

let registeredEarly = true;
let runnersAge = 17;

if (registeredEarly && runnersAge >=18) {
  raceNumber +=1000
} else {
  raceNumber
};

if (runnersAge >=18 && registeredEarly) {
  console.log(`${raceNumber} your face will begin at 9:30am`)
} else if (registeredEarly = false && runnersAge > 18) {
  console.log(`${raceNumber} your face will begin at 11:00am`)
} else {
  console.log('Youth registrants run at 12:30pm (regardless of registration)')
}
